	Doi = {10.1002/bimj.201100041},
	Issn = {0323-3847},
	Journal = {Biometrical Journal},
	Keywords = {NESTED LAPLACE APPROXIMATIONS; LINEAR MIXED MODELS; BAYESIAN-INFERENCE; ACTIVATION; FMRI; VOLUNTARY; FATIGUE; CONTRACTION; PERFORMANCE; REGRESSION},
	Number = {2},
	Pages = {264--280},
	Publisher = {WILEY-BLACKWELL},
	Times-Cited = {6},
	Title = {Joint generalized models for multidimensional outcomes: A case study of neuroscience data from multimodalities},
	Volume = {54},
	Wok-Uid = {WOS:000303045200007},
