	Keywords = {Linear Models;Confidence Intervals;Tumor Markers, Biological;Sensitivity and Specificity;Models, Statistical;comparative study;Markov Chains;Bias (Epidemiology);Meta-Analysis as Topic;Monte Carlo Method;Bayes Theorem;Humans;Biostatistics;Urinary Bladder Neoplasms;Likelihood Functions;Diagnostic Tests, Routine;Telomerase},
	Month = {5},
